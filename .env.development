# just a flag
ENV = 'development'

VUE_APP_PROJECT_NAME = 'saas-web'

# 后端为 saas-cloud 项目： http://127.0.0.1:8760 表示saas-gateway-server服务的访问地址，非本机访问，请配置内网ip
# 后端为 saas-boot 项目： http://127.0.0.1:8760 表示saas-authority-server服务的访问地址，非本机访问，请配置内网ip
VUE_APP_DEV_REQUEST_DOMAIN_PREFIX = 'http://192.168.1.84:8760'

# 请求域名前缀， 该变量仅仅生产环境需要设置
VUE_APP_PROD_REQUEST_DOMAIN_PREFIX = ''
# URI 前缀，用于根据URI前缀进行代理
VUE_APP_BASE_API = '/api'
# 是否启用多租户   NONE DATASOURCE SCHEMA COLUMN
VUE_APP_IS_MULTI_TENANT_TYPE = DATASOURCE
# 是否启用验证码
VUE_APP_IS_CAPTCHA = true
# 客户端秘钥
VUE_APP_CLIENT_ID=saas_web
VUE_APP_CLIENT_SECRET=saas_web_secret
VUE_APP_SVG = '@/icons/svg'

# vue-cli uses the VUE_CLI_BABEL_TRANSPILE_MODULES environment variable,
# to control whether the babel-plugin-dynamic-import-node plugin is enabled.
# It only does one thing by converting all import() to require().
# This configuration can significantly increase the speed of hot updates,
# when you have a large number of pages.
# Detail:  https://github.com/vuejs/vue-cli/blob/dev/packages/@vue/babel-preset-app/index.js

VUE_CLI_BABEL_TRANSPILE_MODULES = true
